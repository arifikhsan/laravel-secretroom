{
    "name": "arifikhsan/laravel-secretroom",
    "description": "Beautiful, automated, dynamic, Laravel admin panel with zero configuration.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel",
        "administration",
        "automation",
        "php",
        "admin",
        "panel"
    ],
    "authors": [
        {
            "name": "Arif Ikhsanudin",
            "email": "arif.ikhsanudin.id@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Arifikhsan\\LaravelSecretroom\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arifikhsan\\LaravelSecretroom\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arifikhsan\\LaravelSecretroom\\LaravelSecretroomServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
